[
  {
    "type": "noise-function",
    "name": "rpi",
    "parameters": [
      "survival"
    ],
    "expression": "random_penalty{x = x, y = y, source = 1, amplitude = 1/survival} - 1"
  },
  {
    "type": "noise-function",
    "name": "decorative_mix_noise",
    "parameters": [
      "seed",
      "input_scale"
    ],
    "expression": "-0.25\\z\n    + abs(multioctave_noise{x = x, y = y, persistence = 0.65, seed0 = map_seed, seed1 = seed + 500, octaves = 2, input_scale = input_scale, output_scale = 0.8, offset_y = seed})\\z\n    + basis_noise{x = x, y = y, seed0 = map_seed, seed1 = seed, input_scale = input_scale, output_scale = 0.1, offset_x = seed}\\z\n    + basis_noise{x = x, y = y, seed0 = map_seed, seed1 = seed, input_scale = input_scale / 2, output_scale = 0.15, offset_x = seed}\\z\n    + basis_noise{x = x, y = y, seed0 = map_seed, seed1 = seed, input_scale = input_scale / 4, output_scale = 0.2, offset_x = seed}"
  },
  {
    "type": "noise-expression",
    "name": "decorative_knockout",
    "expression": "multioctave_noise{x = x, y = y, persistence = 0.7, seed0 = map_seed, seed1 = 1300000, octaves = 2, input_scale = 1/2.5}"
  },
  {
    "type": "noise-expression",
    "name": "grass_noise",
    "expression": "decorative_mix_noise{seed = 1000, input_scale = 1/22}"
  },
  {
    "type": "noise-expression",
    "name": "pita_noise",
    "expression": "decorative_mix_noise{seed = 2000, input_scale = 1/30}"
  },
  {
    "type": "noise-expression",
    "name": "croton_noise",
